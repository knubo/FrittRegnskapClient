#!/bin/sh
APPDIR=`dirname $0`;
GWT_ROOT='/Users/knuterikborgen/Documents/workspacephp/gwt-mac-0.0.2415'
CP="$APPDIR/src:$APPDIR/bin:$GWT_ROOT/gwt-user.jar:$GWT_ROOT/gwt-dev-mac.jar"

java -Xmx256m -XstartOnFirstThread -cp "$CP" com.google.gwt.dev.GWTCompiler -out "$APPDIR/www" "$@" no.knubo.accounting.Login;
