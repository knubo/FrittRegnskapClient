<module rename-to='prg'>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name='com.google.gwt.json.JSON' />
	<inherits name="com.google.gwt.http.HTTP" />

    <inherits name='org.gwtwidgets.WidgetLibrary'/>
    <inherits name='com.google.gwt.visualization.Visualization'/>
	<inherits name='net.binarymuse.gwt.Wizard' />

	<!-- Specify the app entry point class.                   -->
	<entry-point class='no.knubo.accounting.client.AccountingGWT' />


    <inherits name="com.google.gwt.user.theme.chrome.Chrome"/>

  	<stylesheet src='accountstyleNew.css' />
	<stylesheet src='RichTextToolbar.css' />
	

	<!-- Norwegian language, independent of country -->
	<extend-property name="locale" values="no" />

	<!-- English language, independent of country -->
	<extend-property name="locale" values="en" />
</module>
